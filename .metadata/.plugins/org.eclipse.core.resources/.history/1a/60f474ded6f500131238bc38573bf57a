	public RestartButton(){
	String picPath = "/Buttons/Restart.png";
		try{
			pic = ImageIO.read(getClass().getResourceAsStream(arg0))
		}catch(Exception e){
			e.printStackTrace();
		}
		
	}
	
	
	
}
